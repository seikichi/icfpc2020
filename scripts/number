#!/usr/bin/env python3

s = [
 ' #######',
 '##      ',
 '#  ###  ',
 '###  ## ',
 '#   ### ',
 '##  #   ',
 '# #     ',
 '#       ',
 '#       ',
]

def main():
    w = len(s[0]) - 1
    h = len(s) - 1
    sum = 0
    d = 1
    for y in range(1, h+1):
        for x in range(1, w+1):
            if s[y][x] == '#':
                sum += d
            d *= 2
    if w != h:
        sum = -sum
    print(sum)

main()
